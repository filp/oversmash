{
  "name": "oversmash",
  "version": "1.6.1",
  "license": "MIT",
  "homepage": "https://github.com/filp/oversmash",
  "description": "API wrapper for Blizzard's Overwatch player data",
  "main": "./build/index.js",
  "keywords": [
    "overwatch",
    "gaming",
    "api",
    "client",
    "scraper",
    "oversmash"
  ],
  "scripts": {
    "start": "babel-node lib/index.js",
    "test": "DEBUG=oversmash:* babel-node tests/test_oversmash.js",
    "snapshot": "DEBUG=oversmash:snapshot babel-node tests/data_snapshot.js",
    "build": "rm -rf ./build && babel lib -d build",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "camelcase": "^7.0.0",
    "cheerio": "^1.0.0-rc.5",
    "debug": "^4.1.1",
    "no-case": "^3.0.4",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "object-path": "^0.11.5",
    "prettier": "^2.7.1",
    "standard": "^17.0.0",
    "traverse": "^0.6.6",
    "typescript": "^4.8.4"
  }
}
